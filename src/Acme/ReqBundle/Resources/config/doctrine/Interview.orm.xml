<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\ReqBundle\Entity\Interview" table="interview">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="question" type="string" column="question" length="150" nullable="true"/>
    <field name="answer" type="text" column="answer" nullable="true"/>
    <field name="position" type="bigint" column="position" nullable="true"/>
    <field name="topicsOld" type="string" column="topics_old" length="150" nullable="true"/>
  </entity>
</doctrine-mapping>
